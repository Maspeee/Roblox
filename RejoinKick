--	Services  ------------------------------------
--------------------------------------------------

local TpS = game:GetService("TeleportService")
local Core = game:GetService("CoreGui")

--	Variables  -----------------------------------
--------------------------------------------------

local placeId = game.PlaceId
local jobId = game.JobId

--	Main  ----------------------------------------
--------------------------------------------------

function onKick(obj)
    if obj.Name ~= "ErrorPrompt" then return end
    
    local success

    while not success do
        queue_on_teleport(game:HttpGet("https://raw.githubusercontent.com/Maspeee/Roblox/Shared/RejoinKick"))
        success = pcall(TpS.TeleportToPlaceInstance, TpS, placeId, jobId)
    end
end

--------------------------------------------------

Core.RobloxPromptGui.promptOverlay.ChildAdded:Connect(onKick)
